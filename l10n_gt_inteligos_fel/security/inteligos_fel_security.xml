<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="group_fel_user" model="res.groups">
        <field name="name">Usuarios FEL</field>
        <field name="comment">Grupo para usuarios con permisos límitados para las operaciones con FEL dentro de las configuraciones Odoo, sí pueden publicar DTEs.</field>
        <field name="category_id" ref="base.module_category_accounting_accounting"/>
        <field name="implied_ids" eval="[(4, ref('account.group_account_invoice'))]"/>
    </record>

    <record id="group_fel_manager" model="res.groups">
        <field name="name">Administración FEL</field>
        <field name="comment">Grupo para usuarios con todos los permisos para las operaciones con FEL dentro de las configuraciones y operaciones Odoo.</field>
        <field name="category_id" ref="base.module_category_accounting_accounting"/>
        <field name="implied_ids" eval="[(4, ref('group_fel_user'))]"/>
    </record>

</odoo>
